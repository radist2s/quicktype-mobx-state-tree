        "chai": "^4.2.0",
        "jest": "^25.2.7",
        "mobx": "^5.15.4",
        "mobx-state-tree": "^3.15.0",
        "mocha": "^10.2.0",
        "ts-jest": "^25.3.1",
        "ts-node": "^8.8.1",
