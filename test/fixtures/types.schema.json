{
    "$id": "types.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "number": {
            "type": "integer"
        },
        "string": {
            "type": "string"
        },
        "date": {
            "type": "string",
            "format": "date-time"
        },
        "enum": {
            "type": "string",
            "enum": ["first", "second", "third"],
            "default": "first"
        }
    }
}
